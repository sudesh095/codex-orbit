#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules/loose-envify/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/node_modules:/mnt/c/Users/kumar/Documents/node_modules:/mnt/c/Users/kumar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules/loose-envify/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/node_modules:/mnt/c/Users/kumar/Documents/node_modules:/mnt/c/Users/kumar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/kumar/Documents/Sudesh/Projects/codex-orbit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
